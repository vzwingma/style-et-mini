{
  "cli": {
    "version": ">= 10.2.1"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_MY_ENVIRONMENT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_MY_ENVIRONMENT": "preview"
      }
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_MY_ENVIRONMENT": "production"
      }
    },    
    "productionStore": {
      "android": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_MY_ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
